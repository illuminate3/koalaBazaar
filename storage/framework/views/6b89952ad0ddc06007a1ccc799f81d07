<script>


    $(".portfolioBox").click(function(){
        $(".reviewBox").hide("drop",{direction:"right"},500);
        $(this).css("margin","0 auto");
        $(this).animate({width:"100%"},1000);
    });

    $(".reviewBox").click(function(){
        $(".portfolioBox").hide();
        $(this).css({"margin":"0 auto","float":"none"});
        $(this).animate({width:"100%"},1000,function(){
            $(this).html();

        });
    });

</script>

